@article{ WOS:000207922400089,
Author = {Mariguela, T. C. and De-Franco, B. and Almeida, T. V. V. and Mendonca,
   F. F. and Gadig, O. B. F. and Foresti, F. and Oliveira, C.},
Title = {Identification of guitarfish species Rhinobatos percellens, R. horkelli,
   and Zapteryx brevirostris (Chondrichthyes) using mitochondrial genes and
   RFLP technique},
Journal = {CONSERVATION GENETICS RESOURCES},
Year = {2009},
Volume = {1},
Number = {1},
Pages = {393-396},
Month = {DEC},
Abstract = {The RFLP represents a fast and non-expensive tool to access species
   identification and can be used by fishery and law enforcement
   authorities to gather data from legal and illegal ray fisheries. In the
   present study, partial sequences of 16S and COI mitochondrial genes were
   submitted to RFLP analysis and the results showed that there is a high
   interspecific variability and no intraspecific polymorphism, making them
   an useful marker for guitarfish identification. All samples of
   Rhinobatidae: Rhinobatos percellens, R. horkelli, and Zapteryx
   brevirostris were positively identified. The main contribution of these
   forensic identification techniques is the possibility of evaluating
   population genetics of species status in order to improve conservation
   plans involving these species.},
